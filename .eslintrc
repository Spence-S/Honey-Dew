{
  extends: airbnb,
  rules: {
    "no-underscore-dangle": off,
    "no-shadow": ["error", { "allow": ["err","done"] }],
    "consistent-return": off,
    "no-param-reassign": off,
    "comma-dangle": off,
    "no-trailing-spaces": ["error", { "skipBlankLines": false }],
    "no-multiple-empty-lines": off
    }
}
